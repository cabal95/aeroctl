<Window x:Class="AeroCtl.UI.FanCurveEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AeroCtl.UI"
        mc:Ignorable="d"
        Title="Fan Curve" Height="450" Width="600">
    <DockPanel>
        <StackPanel DockPanel.Dock="Bottom" Margin="5" HorizontalAlignment="Left">
            <Button Name="applyButton" Width="100" Click="applyButton_OnClick">Apply</Button>
        </StackPanel>
        <Canvas Name="canvas" Margin="20">

        </Canvas>
    </DockPanel>
</Window>
